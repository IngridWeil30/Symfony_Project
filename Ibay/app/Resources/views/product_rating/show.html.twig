{% extends 'base.html.twig' %}

{% block body %}
    <h1>Product_rating</h1>

    <table>
        <tbody>
            <tr>
                <th>Id</th>
                <td>{{ product_Rating.id }}</td>
            </tr>
            <tr>
                <th>Rating</th>
                <td>{{ product_Rating.rating }}</td>
            </tr>
            <tr>
                <th>Comment</th>
                <td>{{ product_Rating.comment }}</td>
            </tr>
        </tbody>
    </table>

    <ul>
        <li>
            <a href="{{ path('product_rating_index') }}">Back to the list</a>
        </li>
        <li>
            <a href="{{ path('product_rating_edit', { 'id': product_Rating.id }) }}">Edit</a>
        </li>
        <li>
            {{ form_start(delete_form) }}
                <input type="submit" value="Delete">
            {{ form_end(delete_form) }}
        </li>
    </ul>
{% endblock %}
